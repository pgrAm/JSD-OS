{
  "configurations": [
    {
      "inheritEnvironments": [
        "vsdev"
      ],
      "name": "clang",
      "includePath": [
        "${workspaceRoot}\\**",
        "${workspaceRoot}\\cpplib\\include"
      ],
      "intelliSenseMode": "windows-clang-x86",
      "compilerSwitches": "-std=c++20 --target=i386-pc-freebsd-elf -nostdlib -nostdinc -Wconversion -Wall"
    }
  ]
}